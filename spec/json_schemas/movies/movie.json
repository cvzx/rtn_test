{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "id": "file:/movies/movie.json#",
  "type": "object",
  "required": ["id", "title", "plot", "created_at"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "integer",
      "min": 1
    },
    "title": {
      "type": "string",
      "minLength": 1
    },
    "plot": {
      "type": "string"
    },
    "created_at": {
      "type": "string"
    }
  }
}
